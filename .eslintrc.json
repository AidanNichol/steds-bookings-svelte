{
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["svelte3"],
  "extends": ["plugin:compat/recommended","eslint:recommended"],
  "overrides": [
    {
      "files": ["**/*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "rules": {},
  "globals": {
    "__filename": true,
    "__dirname": true,
    "process": true
  }
}
